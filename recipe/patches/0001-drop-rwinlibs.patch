From 0aa8f705106a8a19c4c8e6dea40e3c4fd8e2b316 Mon Sep 17 00:00:00 2001
From: Mervin Fansler <fanslerm@mskcc.org>
Date: Tue, 5 Dec 2023 16:44:19 -0500
Subject: [PATCH] drop rwinlibs

---
 src/Makevars.win | 22 +---------------------
 1 file changed, 1 insertion(+), 21 deletions(-)

diff --git a/src/Makevars.win b/src/Makevars.win
index 44e9a49..31bc3e6 100644
--- a/src/Makevars.win
+++ b/src/Makevars.win
@@ -1,21 +1 @@
-RWINLIB = ../windows/proj-$(VERSION)
-
-TARGET = lib$(R_ARCH)
-
-PKG_LIBS =                  \
-    -L$(RWINLIB)/$(TARGET)    \
-    -L$(RWINLIB)/lib$(R_ARCH) \
-    -lproj -lsqlite3 -lcurl -ltiff -lgeotiff -Lwin64
-
-
-all: clean winlibs
-
-winlibs:
- mkdir -p ../inst
- "${R_HOME}/bin${R_ARCH_BIN}/Rscript.exe" --vanilla "../tools/winlibs.R" $(VERSION)
- cp -r $(RWINLIB)/share/proj ../inst/
-
-clean:
-  rm -f $(SHLIB) $(OBJECTS)
-
-.PHONY: all winlibs clean
+PKG_LIBS = $(PKG_LIBS) -lproj -lsqlite3 -lcurl -ltiff -lgeotiff -Lwin64
